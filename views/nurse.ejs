<!DOCTYPE html>
<html>
  <head>
    <title>Nurse</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <table>
      <tr>
        <td>
          <% studentsList.forEach(student => {%>
          <div class="user">
            <div class="name">
              <%= student.name %> <%= student.surname %> <span class="gender"><%= student.gender %> </span> 
            </div> 
            <div class="birdthDate">
              <%= student.health.blood %>
            </div>
            <div class="create">
              <a href="/nurse/<%= student._id %>" class="btn btn3">Update</a>
            </div>
            <div>
              <a onclick="return alert('Yoy can\'t delete record on this level.');"><div class="delete"></div></a>
            </div>
          </div> 
          <%})%>
        </td>
      </tr>
    </table>
   
    <div class="btn">
      <a href="/admin" class="btn-back">Admin</a>
    </div>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/axios/0.24.0/axios.js"></script>
    <script src="/js/main.js"></script>
  </body>
</html>
